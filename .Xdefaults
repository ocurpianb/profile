!.Xdefaults
!'xrdb -merge .Xdefaults' to apply without reloading
!
!{{{    misc
!-------------------------------------------------------------------------------
Xcursor*theme:  neutral
Xcursor*size:   22
!}}}
!{{{    xft
!-------------------------------------------------------------------------------
Xft.dpi:       96
Xft.antialias: true
Xft.autohint:  false
Xft.hinting:   true
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.rgba:      rgb
!}}}
!{{{    urxvt*
!-------------------------------------------------------------------------------
! Misc & Matcher
urxvt*termName:        rxvt-256color
urxvt*loginShell:      true
urxvt*cutchars:        "()*,<>[]{}|'"
urxvt*perl-lib:        /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher
urxvt*url-launcher:    firefox
urxvt*matcher.button:  1
urxvt*print-pipe:      cat > $HOME/$(echo urxvt.dump.$(date +%s))
urxvt*buffered:        false
urxvt*saveLines:       8192
! Appaerance
urxvt*borderless:   true
urxvt*fading:       30
urxvt*depth:        32
urxvt*background:   rgba:0000/0000/0000/DDDD
urxvt*shading:      15
! Next line must be set to false to use real transparency
urxvt*transparent:  false
urxvt*pointerColor: #919191
urxvt*colorUL:      #4682B4
! Fonts
urxvt*font:           xft:xos4 terminus:size=10
urxvt*boldFont:       xft:xos4 terminus:bold:size=10
urxvt*italicFont:     xft:Bitstream Vera Sans Mono:italic:size=10
urxvt*boldItalicFont: xft:Bitstream Vera Sans Mono:bold:italic:size=10
! Scroll
urxvt*scrollBar:        false
urxvt*skipScroll:       false
urxvt*jumpScroll:       true
urxvt*secondaryScroll:  true
urxvt*scrollWithBuffer: true
!! Tango Colors
urxvt*foreground:   white
!urxvt*borderColor:  #151515
urxvt*colorBD:      #FFFFFF
urxvt*cursorColor:  #FFA500
urxvt*cursorColor2: pink
urxvt*color0:       #2E3436
urxvt*color1:       #CC0000
urxvt*color2:       #4E9A06
urxvt*color3:       #C4A000
urxvt*color4:       #3465A4
urxvt*color5:       #75507B
urxvt*color6:       #16989A
urxvt*color7:       #D3D7CF
urxvt*color8:       #555753
urxvt*color9:       #EF2929
urxvt*color10:      #8AE234
urxvt*color11:      #FCE94F
urxvt*color12:      #729FCF
urxvt*color13:      #AD7FA8
urxvt*color14:      #34E2E2
urxvt*color15:      #EEEEEE
!!}}}
!!{{{    xterm*
!!-------------------------------------------------------------------------------
!xterm*termName:           xterm-256color
!xterm*internalBorder:     0
!xterm*loginShell:         true
!xterm*scrollBar:          false
!xterm*cursorBlink:        true
!xterm*metaSendsEscape:    true
!xterm*altSendsEscape:     true
!xterm*geometry:           80x26
!xterm*saveLines:          8192
!xterm*dynamicColors:      on
!xterm*highlightSelection: true
!! Catch URLs
!xterm*charClass: 33:48,35-38:48,39:43,42-47:48,58-59:48,61:48,63-64:48,126:48
!! Appearance
!xterm*utf8:          2
!xterm*eightBitInput: true
!xterm*faceName:      terminus:pixelsize=16
!xterm*cursorColor:   #FFA500
!xterm*background:    #000000
!xterm*foreground:    #dcdccc
!! Tango Color Scheme
!xterm*color0:  #2e3436
!xterm*color1:  #cc0000
!xterm*color2:  #4e9a06
!xterm*color3:  #c4a000
!xterm*color4:  #3465a4
!xterm*color5:  #75507b
!xterm*color6:  #0b939b
!xterm*color7:  #d3d7cf
!xterm*color8:  #555753
!xterm*color9:  #ef2929
!xterm*color10: #8ae234
!xterm*color11: #fce94f
!xterm*color12: #729fcf
!xterm*color13: #ad7fa8
!xterm*color14: #00f5e9
!xterm*color15: #eeeeec
!!}}}
!!
!! vim: set filetype=xdefaults
